<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="myhealth" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:textSize">36sp</item>
        <item name="android:textColor">#004EEC</item>
    </style>
    <style name="Splash" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="Blue" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryVariant">@color/colorPrimary</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="toolbarStyle">@style/WhiteTitle</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>
        <item name="android:statusBarColor" tools:targetApi="l">@color/colorPrimary</item>

    </style>
    <style name="WhiteTitle" parent="@style/Widget.AppCompat.Toolbar">
        <item name="titleTextColor">@color/white</item>
    </style>
</resources>